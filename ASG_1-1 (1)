#include <stdio.h>

int main() {
    printf("\n");
    int num;
    printf("Enter a number: \n");
    scanf("%d", &num);
    if ((num / 2) * 2 == num) {
        printf("\nThe Number is ODD: 0\n");
        printf("The Number is Even: 1\n\n");
    } else {
        printf("\nThe Number is ODD: 1\n");
        printf("The Number is Even: 0\n\n");
    }
    return 0;
    printf("\n\n");
}
